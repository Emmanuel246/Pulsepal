{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "MONGO_URI": "@mongo_uri",
        "SESSION_SECRET": "@session_secret",
        "GEMINI_API_KEY": "@gemini_api_key"
    }
}